          display: none
      &.y-sending
        background-color: #212844
        background-image: asset_url('717.GIF')
        background-size: 60%
        background-repeat: no-repeat
        background-position: center
      &.y-success
        background-color: #212844
        background-image: asset_url('white-cart.svg')
        background-size: 50%
        background-repeat: no-repeat
        background-position: 50% 41%

      &.y-error
