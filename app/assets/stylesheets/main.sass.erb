      .price
        position: relative
        strong
          font:
            family: "Lobster", cursive
            size: 3.75rem
            weight: 400 !important
          color: #212844
          margin-top: 7px
          display: inline-block
        p
          font:
            family: "Lobster", cursive
            size: 1.875rem
            weight: 400 !important
          color: #FF9E15
          text-transform: lowercase
          position: absolute
          top: 69px
          left: 48px
  &:hover
    .price-wrap
      top: -20px
    .information-block
      padding-bottom: 60%
    form
      bottom: 0 !important
