      h2
        font:
          family: "Lobster", cursive
          size: 1.75rem
          weight: 400 !important
        line-height: rem-calc(27)
        display: inline-grid
        letter-spacing: 0.5px
        +my_vertical_align()
        margin: 0
        padding-bottom: 6px
      form
        position: absolute
        left: 0
        bottom: -53px
        right: 0
        height: 53px
        background-color: #FF661B
        +single-transition(bottom)
        .round-input
          width: 35%
          margin-top: 6px
          .input-count
            margin: 0 auto
            float: none
            input
              margin-top: 0!important
        .input
          width: 35%
          float: left
          height: inherit
          input
            padding-left: 0
            padding-right: 0
            text-align: center
            width: 40px
            height: 40px
            margin: 0 auto
            margin-top: 6px
            border-radius: 50%
            -webkit-box-shadow: 0px 0px 6px 0px rgba(0,0,0,0.6)
            -moz-box-shadow: 0px 0px 6px 0px rgba(0,0,0,0.6)
            box-shadow: 0px 0px 6px 0px rgba(0,0,0,0.6)
          input[type=number]::-webkit-inner-spin-button, nput[type=number]::-webkit-outer-spin-button
            -webkit-appearance: none
            margin: 0
        button
          width: 65%
          height: inherit
          background: transparent
          text-transform: uppercase
          text-align: left
          font:
            family: "Fira Sans", sans-serif
            size: rem-calc(21)
            weight: 600
          margin: 0
          line-height: 20px
          letter-spacing: .05rem
          padding-left: 2.5%
    .price-wrap
      width: 110px
      height: 110px
      border-radius: 50%
      background-color: #fff
      position: absolute
      right: 25px
      top: -60px
      -webkit-box-shadow: 0px 0px 6px 0px rgba(0,0,0,0.6)
      -moz-box-shadow: 0px 0px 6px 0px rgba(0,0,0,0.6)
      box-shadow: 0px 0px 6px 0px rgba(0,0,0,0.6)
      z-index: 10
      +single-transition(top)
